TARGET = @PACKAGE_TARNAME@@EXEEXT@
OBJS  = syncTimeICOM.@OBJEXT@
OBJS += common.@OBJEXT@
OBJS += ttyserial.@OBJEXT@
OBJS += tpbar.@OBJEXT@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@

CC = @CC@
CFLAGS = @CFLAGS@
DEFS = @DEFS@
LIBS = @LIBS@
INSTALL = @INSTALL@

VPATH = @srcdir@
@SET_MAKE@

.PHONY: all clean distclean install uninstall

all : $(TARGET)

syncTimeICOM.@OBJEXT@ : syncTimeICOM.c common.h ttyserial.h tpbar.h config.h
common.@OBJEXT@ : common.c common.h config.h
ttyserial.@OBJEXT@ : ttyserial.c ttyserial.h config.h
tpbar.@OBJEXT@ : tpbar.c tpbar.h common.h config.h

$(TARGET) : $(OBJS)
	$(CC) $(CFLAGS) -o $@ $^ $(LIBS)

%.@OBJEXT@ : %.c
	$(CC) $(CFLAGS) $(DEFS) -c $<

clean :
	$(RM) $(TARGET)
	$(RM) $(OBJS)

distclean : clean
	$(RM) config.log config.status config.cache
	$(RM) Makefile config.h

install : $(TARGET) $(DESTDIR)$(bindir)
	$(INSTALL) -m755 -s $^

uninstall :
	$(RM) $(addprefix $(DESTDIR)$(bindir)/,$(notdir $(TARGET)))

$(DESTDIR)$(bindir) :
	$(INSTALL) -m755 -d $@
